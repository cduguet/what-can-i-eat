# Supabase Testing Environment Configuration
# This file configures the app to use Supabase backend for testing

# Supabase Configuration (from .env.example)
EXPO_PUBLIC_SUPABASE_URL=https://YOUR-PROJECT.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY

# Backend Mode Configuration - Set to Supabase
EXPO_PUBLIC_BACKEND_MODE=supabase

# AI Provider Configuration - Use Gemini (we have the API key)
EXPO_PUBLIC_AI_PROVIDER=gemini

# Gemini API Configuration (for reference, will be set in Supabase)
EXPO_PUBLIC_GEMINI_API_KEY=

# Common Configuration
EXPO_PUBLIC_API_TIMEOUT=30000
EXPO_PUBLIC_MAX_RETRIES=3

# Development Configuration
NODE_ENV=development
